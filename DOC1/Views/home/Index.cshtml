<link href="~/Content/bootstrap.css" rel="stylesheet" type="text/css" />






<hr />
<div class="jumbotron" style="background-color:#808080">




    <div id="mostar">
        <table width="100%">
            <tr class="form-group">

                <th class="col-md-2">
                    <p class="centrar">
                        @Html.DisplayNameFor(model => model.VE_TANQ)

                    </p>


                </th>

                <th class="col-md-3">
                    <p class="centrar">
                        @Html.DisplayNameFor(model => model.VE_NOM)
                    </p>

                </th>
                <th class="col-md-3">
                    <p class="centrar">
                        @Html.DisplayNameFor(model => model.VE_AUM)

                    </p>

                </th>
                <th class="col-md-4">
                    <p class="centrar">
                        @Html.DisplayNameFor(model => model.VE_HFI)
                    </p>

                </th>

            </tr>
        </table>


        <div class="list-group" id="menu">

            @foreach (var item in Model)
            {
                <a href="#" class="list-group-item" id="@Html.DisplayFor(modelItem =>item.VE_ID)">

                    <div class="col-md-2" id="valotanq">
                   
                        @Html.DisplayFor(modelItem => item.VE_TANQ)

                    </div>
                    <div class="col-md-3 nombre" id="idnombre">
                        @Html.DisplayFor(modelItem => item.VE_NOM)  
                        @Html.HiddenFor(mdoelItem => item.VE_NOM, new { @id = "nombre"})
            
                    </div>
                    <div class=" col-md-3">
                        @Html.DisplayFor(modelItem => item.VE_AUM)  litros
                        @Html.HiddenFor(modelItem => item.VE_AUM, new { @id = "VE_AUM" })
                    </div>
                    <div>
                        @Html.DisplayFor(modelItem => item.VE_HFI)
                        @Html.HiddenFor(modelItem => item.VE_HFI, new { @id = "VE_HFI" })
                        
                    
                    </div>
                   
                        @Html.HiddenFor(modelItem => item.DATO, new { })

                     

                        @Html.HiddenFor(modelItem => item.VE_HFF,new {@id="VE_HFF" })
                        @Html.HiddenFor(modelItem => item.VE_LTRI,new {@id="VE_LTRI" })
                        @Html.HiddenFor(modelItem => item.VE_LTRF, new {@id = "VE_LTRF"})
                        @Html.HiddenFor(modelItem => item.VE_TANQ,new {@id ="VE_TANQ" })
                        
  
                </a>


            }


        </div>









    </div>





</div>


@Html.Partial("modal", new Send_Carga())







