                </th>

                <th class="col-md-3">
                    <p class="centrar">
                        @Html.DisplayNameFor(model => model.VE_NOM)
                    </p>

                </th>
                <th class="col-md-3">
                    <p class="centrar">
                        @Html.DisplayNameFor(model => model.VE_AUM)

                    </p>

                </th>
                <th class="col-md-4">
                    <p class="centrar">
                        @Html.DisplayNameFor(model => model.VE_HFI)
                    </p>

                </th>

            </tr>
        </table>


        <div class="list-group" id="menu">

            @foreach (var item in Model)
            {
                <a href="#" class="list-group-item" id="@Html.DisplayFor(modelItem =>item.VE_ID)" >
                   
                    
                    <div class="col-md-2" id="valotanq">
                   
                        @Html.DisplayFor(modelItem => item.VE_TANQ)
                        @Html.HiddenFor(modelItem=>item.VE_TANQ, new { @id =item.VE_ID+"VE_TANQ"})
                    </div>
                    <div class="col-md-3 nombre" id="idnombre">
                        @Html.DisplayFor(modelItem => item.VE_NOM)  
                        @Html.HiddenFor(mdoelItem => item.VE_NOM, new { @id = "nombre"})
            
                    </div>
                    <div class=" col-md-3">
                        @Html.DisplayFor(modelItem => item.VE_AUM)  litros
                        @Html.HiddenFor(modelItem => item.VE_AUM, new { @id =item.VE_ID + "VE_AUM" })
                    </div>
                    <div>
                        @Html.DisplayFor(modelItem => item.VE_HFI)
                        @Html.HiddenFor(modelItem => item.VE_HFI, new { @id = item.VE_ID + "VE_HFI" })
                        
                    
                    </div>
                       

                    @Html.HiddenFor(modelItem => item.VE_GCF, new { @id = item.VE_ID + "VE_GCF" })

                       @Html.HiddenFor(modelItem=> item.VE_FOLR, new {@id =item.VE_ID+"VE_FOLR"})
                                   

                        @Html.HiddenFor(modelItem => item.VE_HFF,new {@id= item.VE_ID + "VE_HFF" })
                        @Html.HiddenFor(modelItem => item.VE_LTRI,new {@id= item.VE_ID + "VE_LTRI" })
                        @Html.HiddenFor(modelItem => item.VE_LTRF, new {@id = item.VE_ID + "VE_LTRF" })
                        @Html.HiddenFor(modelItem => item.VE_TANQ,new {@id = item.VE_ID + "VE_TANQ" })
                    @Html.HiddenFor(modelItem => item.V_clvpro, new { @id = item.VE_ID + "V_clvepro" })
                    @Html.HiddenFor(modelItem => item.V_clvsubpro , new { @id = item.VE_ID + "V_clvsubpro" })
                    @Html.HiddenFor(modelItem => item.V_CocGas, new { @id = item.VE_ID + "V_CocGas" })
                    @Html.HiddenFor(modelItem => item.V_GasEtn, new { @id = item.VE_ID + "V_GasEtn" })
                    @Html.HiddenFor(modelItem => item.V_CetnGas, new { @id = item.VE_ID + "V_CetnGas" })
                    @Html.HiddenFor(modelItem => item.V_Otros, new { @id = item.VE_ID + "V_Otros" })
                    @Html.HiddenFor(modelItem => item.V_Marca, new { @id = item.VE_ID + "V_Marca" })
  
                </a>


            }


        </div>


    </div>

</div>


